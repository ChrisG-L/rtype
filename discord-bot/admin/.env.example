# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token_here

# Admin channel ID (private channel for admin commands)
ADMIN_CHANNEL_ID=123456789012345678

# Admin role ID (users with this role can use admin commands)
ADMIN_ROLE_ID=123456789012345678

# R-Type TCP Admin Server Connection
RTYPE_SERVER_HOST=localhost
RTYPE_ADMIN_PORT=4127

# Admin token (MUST match ADMIN_TOKEN on the R-Type server)
ADMIN_TOKEN=your_secure_admin_token_here

# Logging
LOG_LEVEL=INFO
